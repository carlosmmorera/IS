
public class Mensaje {
    private String destinatario;
    private String remitente;
    private String asunto;
    private String cuerpo;
    
    public Mensaje(String para, String de, String asunto, String mensaje){
    	destinatario = para;
    	remitente = de;
    	this.asunto = asunto;
    	cuerpo = mensaje;
    }

    public void mostrar() {
    }

}
